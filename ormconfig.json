{
    "type": "postgres",
    "url": "process.ENV.TYPEORM_URL",
    "entities": [
        "dist/db/*entity.js"
    ],
    "synchronize": "false",
    "migrations": [
        "dist/db/migration/*.js"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "cli": {
        "migrationsDir": "src/db/migration",
        "subscribersDir": "src/subscriber"
    },
    "ssl": true,
    "extra": {
        "ssl": {
            "rejectUnauthorized": false
        }
    }
}